{
  "name": "api-cypress",
  "version": "1.0.0",
  "description": "Proyecto API con Cypress y Javascript",
  "main": "index.js",
  "scripts": {
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:headed": "cypress run --headed",
    "test": "cypress run",
    "test:api": "cypress run --spec 'cypress/e2e/api/**/*'",
    "test:smoke": "cypress run --env grep=@smoke",
    "test:sad-path": "cypress run --env grep=@sad-path",
    "test:happy-path": "cypress run --env grep=@happy-path",
    "test:api-gorest": "cypress run --env grep=@gorest",
    "test:create": "cypress run --env grep=@create",
    "test:user": "cypress run --env grep=@user",
    "test:interfaz": "cypress run --env grep=@interfaz",
    "test:parametros": "cypress run --env grep=@parametros"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manu-salvador/api-cypress.js.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/manu-salvador/api-cypress.js/issues"
  },
  "homepage": "https://github.com/manu-salvador/api-cypress.js#readme",
  "devDependencies": {
    "@cypress/grep": "^4.1.0",
    "cypress": "^14.5.3"
  }
}
